{
  "expo": {
    "name": "renDVApp",
    "slug": "renDVApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rendv_images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/rendv_images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#91cfc1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber":"1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rendv_images/adaptive-icon.png",
        "backgroundColor": "#91cfc1"
      },"versionCode":1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
